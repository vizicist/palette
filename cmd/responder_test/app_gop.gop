package main

import (
	"log"

	"github.com/vizicist/palette/engine"
)

func callback(eventType string, eventData string) {
	log.Printf("example_gop: callback type=%s data=%s\n", eventType, eventData)
}

func main() {
	engine.InitLog("example_gop")
	plugin, err := engine.PluginRegister("all", callback)
	if err != nil {
		log.Printf("plugin.Register: err=%s\n", err)
		return
	}
	select {} // block forever
}

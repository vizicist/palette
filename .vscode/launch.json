    "version": "0.2.0",
    "configurations": [
        {
            "name": "Engine",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${env:PALETTESOURCE}/cmd/palette_engine/palette_engine.go",
            "env": {
                "Path": "${env:Path};${workspaceFolder};c:/Users/me/Documents/github/vizicist/palette/SenselLib/x64;"             },
            "args": []
        },
        {
            "name": "start allsmall",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${env:PALETTESOURCE}/cmd/palette/palette.go",
            "env": {
                "Path": "${env:Path};${workspaceFolder};c:/Users/me/Documents/github/vizicist/palette/SenselLib/x64;"             },
            "args": ["start", "allsmall"]
        },
        {
            "name": "stop all",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${env:PALETTESOURCE}/cmd/palette/palette.go",
            "env": {
                "Path": "${env:Path};${workspaceFolder};c:/Users/me/Documents/github/vizicist/palette/SenselLib/x64;"             },
            "args": ["stop", "all"]
        },
        {
            "name": "Launch migrateeffects",
            "type": "python",
            "request": "launch",
            "program": "${env:PALETTESOURCE}/python/migrateeffects.py",
            "env": {},
            "args": []
        },
        {
            "name": "Launch changeparamval",
            "type": "python",
            "request": "launch",
            "program": "${env:PALETTESOURCE}/python/changeparamval.py",
            "env": {},
            "args": ["default\\presets\\visual", "aspect", "1.000", "1.000"]
        }
    ]
}

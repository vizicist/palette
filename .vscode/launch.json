{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Engine",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${env:USERPROFILE}/go/src/github.com/vizicist/palette/cmd/palette_engine/palette_engine.go",
            "env": {
                "Path": "${env:Path};${workspaceFolder};%{env:PALETTE}"             },
            "args": []
        },
        {
            "name": "Launch migrateeffects",
            "type": "python",
            "request": "launch",
            "program": "${env:USERPROFILE}/go/src/github.com/vizicist/palette/python/migrateeffects.py",
            "env": {},
            "args": []
        },
        {
            "name": "Launch changeparamval",
            "type": "python",
            "request": "launch",
            "program": "${env:USERPROFILE}/go/src/github.com/vizicist/palette/python/changeparamval.py",
            "env": {},
            "args": ["default\\presets\\visual", "aspect", "1.000", "1.000"]
        },
        {
            "name": "Launch GUI",
            "type": "python",
            "request": "launch",
            "program": "${env:USERPROFILE}/go/src/github.com/vizicist/palette/python/palette_gui.py",
            "env": {},
            "args": []
        }
    ]
}

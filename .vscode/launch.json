            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${env:PALETTESOURCE}/cmd/palette_engine/palette_engine.go",
            "env": {
                "Path": "${env:Path};${workspaceFolder};c:/Users/me/Documents/github/vizicist/palette/SenselLib/x64;"             },

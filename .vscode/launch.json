    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Engine",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${env:USERPROFILE}/go/src/github.com/vizicist/palette/cmd/palette_engine/palette_engine.go",
            "env": {
                "Path": "${env:Path};${workspaceFolder};%{env:PALETTE}"             },
            "args": []
        },
        {
            "name": "Launch midifile",
            "type": "python",
            "request": "launch",
            "program": "${env:USERPROFILE}/go/src/github.com/vizicist/palette/python/palette_midifile.py",
            "env": {},
            "args": ["cdef.mid","debug"]
        },
        {
            "name": "Launch GUI Full",
            "type": "python",
            "request": "launch",
            "program": "${env:USERPROFILE}/go/src/github.com/vizicist/palette/python/palette_gui_full.py",
            "env": {},
            "args": []
        },
        {
            "name": "Launch GUI Remote",
            "type": "python",
            "request": "launch",
            "program": "${env:USERPROFILE}/go/src/github.com/vizicist/palette/python/palette_gui_remote.py",
            "env": {},
            "args": []
        },
        {
            "name": "Launch GUI Viz",
            "type": "python",
            "request": "launch",
            "program": "${env:USERPROFILE}/go/src/github.com/vizicist/palette/python/palette_gui_viz.py",
            "env": {},
            "args": []
        }
    ]
}

            "cwd": "${env:PALETTESOURCE}/kit",
            "env": {
                "Path": "${env:Path};${workspaceFolder};"             },
            "args": []
        },
        {
            "name": "testcli",
            "type": "go",
            "request": "launch",
            "program": "${env:PALETTESOURCE}/cmd/palette/palette.go",
            // "args": ["start"]
            // "args": ["stop"]
            // "args": ["-region=A", "save", "snap.default"]
            // "args": ["save", "quad.default"]

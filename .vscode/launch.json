    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Engine+GUI",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${env:USERPROFILE}/go/src/github.com/vizicist/palette/cmd/palette_engine/palette_engine.go",
            "env": {
                "Path": "${env:Path};${workspaceFolder};%{env:PALETTE}"             },
            "args": []
        },
        {
            "name": "Launch ISF",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${env:USERPROFILE}/go/src/github.com/vizicist/palette/cmd/palette_isf/palette_isf.go",
            "env": {
                "Path": "${env:Path};${workspaceFolder};%{env:PALETTE}"             },
            "args": []
        },
        {
            "name": "Launch Python GUI",
            "type": "python",
            "request": "launch",
            "program": "${env:USERPROFILE}/go/src/github.com/vizicist/palette/python/palette_gui.py",
            "env": {},
            "args": []
        }
    ]
}

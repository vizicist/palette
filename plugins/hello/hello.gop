package main

import (
	"github.com/vizicist/palette/plugin"
	"strings"
)

func callback(eventType string, eventData string) {
	println "hello: callback type=%s data=%s\n", eventType, eventData
}

func main() {
	err := plugin.Register("hello", plugin.EventsAll, callback)
	if err != nil {
		println "plugin.Register: err=%s\n", err
		return
	}
	select {} // block forever
}

dircmp . "/c/Program Files/Common Files/Palette/data/config" | grep different | sed -e "s/different[ \t]*//" > tmp.out
if [ ! -s tmp.out ] ; then
	echo "NO DIFFERENCES!"
	exit 0
fi
echo "DIFFERENCES:"
cat tmp.out
echo -n "Do you want to copy them to Common Files? (y or n) > "
read a
echo a=$a
if [ "$a" = "y" ] ; then
	set -x
	cp `cat tmp.out` "/c/Program Files/Common Files/Palette/data/config"
else
	echo "not copied!"
fi
